<% layout('./layouts/boilerplate') %>

<style>
    .track-order {
        max-width: 600px;
        margin: 50px auto;
        padding: 20px;
        background-color: #fff;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        border-radius: 10px;
        text-align: center;
    }

    .track-order h1 {
        font-size: 2.5em;
        color: #1a1f71;
        margin-bottom: 20px;
    }

    .track-order p {
        color: #666;
        font-size: 1.1em;
        margin-bottom: 30px;
    }

    .track-order .input-group {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: 10px;
        margin-bottom: 20px;
    }

    .track-order input[type="text"] {
        padding: 10px;
        font-size: 1.2em;
        width: 70%;
        border: 2px solid #ccc;
        border-radius: 5px;
        transition: border-color 0.3s;
    }

    .track-order input[type="text"]:focus {
        border-color: #1a1f71;
    }

    .track-order button {
        background-color: #1a1f71;
        color: #fff;
        padding: 10px 20px;
        font-size: 1.2em;
        border: none;
        border-radius: 5px;
        cursor: pointer;
        transition: background-color 0.3s;
    }

    .track-order button:hover {
        background-color: #ff8c00;
    }

    .track-order .order-status {
        text-align: left;
        margin-top: 30px;
    }

    .track-order h2, .track-order h3 {
        color: #1a1f71;
    }

    .track-order ul {
        list-style-type: none;
        padding: 0;
    }

    .track-order ul li {
        background-color: #f0f0f0;
        margin-bottom: 10px;
        padding: 10px;
        border-radius: 5px;
        color: #333;
    }

    .track-order .tracking-image {
        max-width: 150px;
        margin-bottom: 20px;
    }
</style>

<body>
    <main>
        <div class="track-order">
            <h1>Track Your Order</h1>
            <p>Enter your order ID below to track the status of your order.</p>

            <div class="form-section">
                <img src="data:image/png;base64," alt="Tracking Icon" class="tracking-image"> <!-- Image added here -->
                <form id="trackOrderForm">
                    <div class="input-group">
                        <input type="text" id="orderId" placeholder="Enter your Order ID" required>
                        <button type="submit">Track Order</button>
                    </div>
                </form>
            </div>

            <div id="orderStatus" class="order-status" style="display: none;">
                <h2>Order Status</h2>
                <p id="statusMessage"></p>
                <h3>Order Details</h3>
                <ul id="orderDetails"></ul>
            </div>
        </div>
    </main>
    <script src="track_order.js"></script>
</body>
